<wxs src="../../utils/analysis-time.wxs" module="analysisTime"></wxs>

<view class="nav">
	<view class="content">
		<view class="header">
			<view>----------预订成功----------</view>
		</view>
		<view class="details">
			<block class="block-details" wx:for="{{successInfo}}" wx:key="index">
				<view>{{index+1}}    id:{{item.objectId}}</view>
				<view>资源名称：{{item.name}}</view>
				<view>日期：{{item.date}}</view>
				<view>时间：{{analysisTime.handaltime(item.time)}}</view>
				<view class="cancelBtn" bindtap="cancelTap">退订</view>
			</block>
		</view>
	</view>


	<view class="content">
		<view class="header">
			<view>----------正在审核----------</view>
		</view>
		<view class="details">
			<block class="block-details" wx:for="{{examiningInfo}}" wx:key="index">
				<view>{{index+1}}</view>
				<view>资源名称：{{item.name}}</view>
				<view>日期：{{item.date}}</view>
				<view>时间：{{analysisTime.handaltime(item.time)}}</view>
				<view class="cancelBtn" bindtap="cancelTap">退订</view>
			</block>
		</view>
	</view>

	<view class="content">
		<view class="header">
			<view>----------已退订/未通过----------</view>
		</view>
		<view class="details">
			<block class="block-details" wx:for="{{failInfo}}" wx:key="index">
				<view>{{index+1}}</view>
				<view>资源名称：{{item.name}}</view>
				<view>日期：{{item.date}}</view>
				<view>时间：{{analysisTime.handaltime(item.time)}}</view>
			</block>
		</view>
	</view>


	<view class="content">
		<view class="header">
			<view>----------使用完成----------</view>
		</view>
		<view class="details">
			<block class="block-details" wx:for="{{completedInfo}}" wx:key="index">
				<view>{{index+1}}</view>
				<view>资源名称：{{item.name}}</view>
				<view>日期：{{item.date}}</view>
				<view>时间：{{analysisTime.handaltime(item.time)}}</view>
				<view>信用积分变化：{{item.markChange}}</view>
			</block>
		</view>
	</view>




</view>
