<!--pages/personal/personal.wxml-->
<view class="personalWrap">
	<view class="personalMe">
		<image src="/images/默认头像.png" style="width:68px;height:68px;border:1px solid #ddd;float:left;margin:20rpx 20rpx;border-radius: 50%;"></image>
		<view class="personalInfo">
			<text>姓名: {{userinfo.username}}</text>
			<text>身份: 普通用户</text>

		</view>
	</view>
	<!-- 列表 -->
	<view class="personalList">
		<view bindtap="listmybook">预定/退订</view>
		<view bindtap="listchangeperinfo">修改个人信息</view>
	</view>
	<view class="exit">
		<button bindtap="exitlogin">退出登录</button>
	</view>
</view>
<import src="../../template/template.wxml" />
<template is="qrcode" />